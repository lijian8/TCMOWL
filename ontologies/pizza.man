Annotations: 
    owl:versionInfo "version 1.5"^^xsd:string,
    owl:versionInfo "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional",
    rdfs:comment "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)",
    owl:versionInfo "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"

AnnotationProperty: owl:versionInfo

    
AnnotationProperty: rdfs:label

    
AnnotationProperty: rdfs:comment

    
Datatype: rdf:PlainLiteral

    
Datatype: xsd:string

    
ObjectProperty: hasBase

    SubPropertyOf: 
        hasIngredient
    
    Characteristics: 
        InverseFunctional,
        Functional
    
    Domain: 
        Pizza
    
    Range: 
        PizzaBase
    
    InverseOf: 
        isBaseOf
    
    
ObjectProperty: hasSpiciness

    Annotations: 
        rdfs:comment "A property created to be used with the ValuePartition - Spiciness."
    
    Characteristics: 
        Functional
    
    Range: 
        Spiciness
    
    
ObjectProperty: hasIngredient

    Annotations: 
        rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"
    
    Characteristics: 
        Transitive
    
    Domain: 
        Food
    
    Range: 
        Food
    
    InverseOf: 
        isIngredientOf
    
    
ObjectProperty: hasTopping

    Annotations: 
        rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"
    
    SubPropertyOf: 
        hasIngredient
    
    Characteristics: 
        InverseFunctional
    
    Domain: 
        Pizza
    
    Range: 
        PizzaTopping
    
    InverseOf: 
        isToppingOf
    
    
ObjectProperty: isBaseOf

    SubPropertyOf: 
        isIngredientOf
    
    Characteristics: 
        InverseFunctional,
        Functional
    
    Domain: 
        PizzaBase
    
    Range: 
        Pizza
    
    InverseOf: 
        hasBase
    
    
ObjectProperty: isIngredientOf

    Annotations: 
        rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."
    
    Characteristics: 
        Transitive
    
    Domain: 
        Food
    
    Range: 
        Food
    
    InverseOf: 
        hasIngredient
    
    
ObjectProperty: isToppingOf

    Annotations: 
        rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"
    
    SubPropertyOf: 
        isIngredientOf
    
    Characteristics: 
        Functional
    
    Domain: 
        PizzaTopping
    
    Range: 
        Pizza
    
    InverseOf: 
        hasTopping
    
    
ObjectProperty: hasCountryOfOrigin

    
